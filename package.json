{
  "name": "traceflow",
  "version": "0.1.1",
  "description": "Track app scenarios with precision using telemetry markers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "tsc",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
